echo "bench,bpred,size,metric,value" > compiled_results/bimod.csv
ls results/*bimod*.txt | xargs -I % sh -c 'grep -H "bpred_.*" %' | sed -r 's|results/||g' | sed -r 's|\..*:|,|' | sed -r 's|bpred_[^\_]+\.||g' | sed -r 's|\s+#.*||g' | sed -r 's|_bimod_|,bimod,|g' | sed -r 's|\s+|,|g' | sed -r 's|[,]+|,|g' >> compiled_results/bimod.csv
ls results/*bimod*.txt | xargs -I % sh -c 'grep -H "sim_.*" %' | sed -r 's|results/||g' | sed -r 's|\..*:|,|' | sed -r 's|bpred_[^\_]+\.||g' | sed -r 's|\s+#.*||g' | sed -r 's|_bimod_|,bimod,|g' | sed -r 's|\s+|,|g' | sed -r 's|[,]+|,|g' >> compiled_results/bimod.csv
