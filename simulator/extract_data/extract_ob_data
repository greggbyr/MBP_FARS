echo "bench,bpred,size,metric,value" > compiled_results/ob.csv
ls results/*ob*.txt | xargs -I % sh -c 'grep -H "bpred_.*.bpred_addr_rate" %' | sed -r 's|results/||g' | sed -r 's|\..*:|,|' | sed -r 's|bpred_[^\_]+\.||g' | sed -r 's|\s+#.*||g' | sed -r 's|_ob_|,ob,|g' | sed -r 's|\s+|,|g' | sed -r 's|[,]+|,|g' >> compiled_results/ob.csv
