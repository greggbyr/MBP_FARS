ls results/*_tsbp_*.txt | grep -v disabled | xargs -I % sh -c 'grep -H "bpred_tsbp.bpred_addr_rate" %' > compiled_results/bpred_tsbp.bpred_addr_rate.csv
ls results/*_tsbp_*.txt | grep -v disabled | xargs -I % sh -c 'grep -H "bpred_tsbp.bpred_dir_rate" %' > compiled_results/bpred_tsbp.bpred_dir_rate.csv
