ls results/*_tsbp_disabled_*.txt | xargs -I % sh -c 'grep -H "bpred_tsbp_disabled.bpred_addr_rate" %' > compiled_results/bpred_tsbp_disabled.bpred_addr_rate.csv
ls results/*_tsbp_disabled_*.txt | xargs -I % sh -c 'grep -H "bpred_tsbp_disabled.bpred_dir_rate" %' > compiled_results/bpred_tsbp_disabled.bpred_dir_rate.csv
ls results/*_tsbp_disabled_*.txt | xargs -I % sh -c 'grep -H "bpred_tsbp_disabled.bpred_jr_rate" %' > compiled_results/bpred_tsbp_disabled.bpred_jr_rate.csv
ls results/*_tsbp_disabled_*.txt | xargs -I % sh -c 'grep -H "bpred_tsbp_disabled.bpred_jr_non_ras_rate.PP" %' > compiled_results/bpred_tsbp_disabled.bpred_jr_non_ras_rate.PP.csv
ls results/*_tsbp_disabled_*.txt | xargs -I % sh -c 'grep -H "bpred_tsbp_disabled.ras_rate.PP" %' > compiled_results/bpred_tsbp_disabled.ras_rate.PP.csv
ls results/*_tsbp_disabled_*.txt | xargs -I % sh -c 'grep -H "bpred_tsbp_disabled.misses" %' > compiled_results/bpred_tsbp_disabled.misses.csv
ls results/*_tsbp_disabled_*.txt | xargs -I % sh -c 'grep -H "sim_num_insn" %' > compiled_results/bpred_tsbp_disabled.sim_num_insn.csv
