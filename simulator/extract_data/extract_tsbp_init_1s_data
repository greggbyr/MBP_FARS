ls results/*_tsbp_init_1s_*.txt | xargs -I % sh -c 'grep -H "bpred_tsbp.bpred_jr_non_ras_rate.PP" %' > compiled_results/bpred_tsbp_init_1s.bpred_jr_non_ras_rate.PP.csv
ls results/*_tsbp_init_1s_*.txt | xargs -I % sh -c 'grep -H "bpred_tsbp.ras_rate.PP" %' > compiled_results/bpred_tsbp_init_1s.ras_rate.PP.csv
