ls results/*_tsbp_init_1s_*.txt | xargs -I % sh -c 'grep -H "bpred_tsbp.bpred_addr_rate" %' > compiled_results/bpred_tsbp_init_1s.bpred_addr_rate.csv
ls results/*_tsbp_init_1s_*.txt | xargs -I % sh -c 'grep -H "bpred_tsbp.bpred_dir_rate" %' > compiled_results/bpred_tsbp_init_1s.bpred_dir_rate.csv
