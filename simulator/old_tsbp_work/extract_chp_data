echo "bench,bpred,size,metric,value" > compiled_results/chp.csv
ls results/*chp*.txt | grep -P "chp_[^\_]+\.txt" | xargs -I % sh -c 'grep -H "bpred_.*.bpred_addr_rate" %' | sed -r 's|results/||g' | sed -r 's|\..*:|,|' | sed -r 's|bpred_[^\_]+\.||g' | sed -r 's|\s+#.*||g' | sed -r 's|_chp_|,chp,|g' | sed -r 's|\s+|,|g' | sed -r 's|[,]+|,|g' >> compiled_results/chp.csv
ls results/*chp*.txt | grep -P "chp_[^\_]+\.txt" | xargs -I % sh -c 'grep -H "bpred_.*.bpred_dir_rate" %' | sed -r 's|results/||g' | sed -r 's|\..*:|,|' | sed -r 's|bpred_[^\_]+\.||g' | sed -r 's|\s+#.*||g' | sed -r 's|_chp_|,chp,|g' | sed -r 's|\s+|,|g' | sed -r 's|[,]+|,|g' >> compiled_results/chp.csv
